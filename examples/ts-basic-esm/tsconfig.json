{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "#/*": ["./src/*"],
      "#prisma/client": ["./prisma/client"]
    }
  },
  "include": ["src/**/*", "prisma/**/*"]
}
