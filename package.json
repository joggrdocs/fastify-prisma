{
  "name": "@joggr/fastify-prisma",
  "version": "1.0.4",
  "description": "Fastify Prisma plugin connection to share the same connection across your entire server.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint-staged": "lint-staged",
    "type:check": "tsc --noEmit",
    "lint:check": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "package": "node ./scripts/package.js",
    "package:increment": "node ./scripts/package-source-increment.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joggrdocs/fastify-prisma.git"
  },
  "keywords": [
    "fastify",
    "plugin",
    "prisma",
    "fastify-plugin"
  ],
  "author": "Joggr Engineering <engineering@joggr.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joggrdocs/fastify-prisma/issues"
  },
  "homepage": "https://github.com/joggrdocs/fastify-prisma#readme",
  "devDependencies": {
    "@prisma/client": "^5.3.1",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.1.0",
    "fastify": "^4.23.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "fastify-plugin": "^4.5.1"
  },
  "peerDependencies": {
    "@prisma/client": "4.x || 5.x"
  }
}
